<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="una.ac.cr.git">

    <!-- PERMISOS PARA VERSION NUEVA DE ANDROID Y LA VIEJA -->

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <!-- PARA VERSIONES VIEJAS DE ANDROID -->

    <uses-feature android:name="android.hardware.location.gps" />

    <!-- PERMISOS DE LLAMDA -->
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Login"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".MainActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".FormUsuario"
            android:screenOrientation="portrait" />
        <activity
            android:name=".PerfilUsuario"
            android:screenOrientation="portrait" />
        <activity
            android:name=".HistorialTomas"
            android:screenOrientation="portrait" />
        <activity
            android:name=".TipsSalud"
            android:screenOrientation="portrait" />
        <activity
            android:name=".NumEmergencias"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Acerca"
            android:screenOrientation="portrait" />


    </application>

</manifest>